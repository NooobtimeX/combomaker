{
  "plugins": ["prettier-plugin-tailwindcss"],
  "arrowParens": "always",
  "semi": true,
  "experimentalTernaries": false,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "singleAttributePerLine": false,
  "vueIndentScriptAndStyle": false,
  "proseWrap": "preserve",
  "insertPragma": false,
  "printWidth": 80,
  "requirePragma": false,
  "tabWidth": 2,
  "useTabs": false,
  "embeddedLanguageFormatting": "auto"
}
